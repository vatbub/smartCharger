<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.*?>
<GridPane xmlns:fx="http://javafx.com/fxml/1" hgap="5.0" maxHeight="-Infinity" maxWidth="-Infinity"
          minHeight="-Infinity" minWidth="-Infinity" prefHeight="400.0" prefWidth="600.0" vgap="5.0"
          xmlns="http://javafx.com/javafx/8" fx:controller="com.github.vatbub.smartcharge.ProfileConditionMainView">
    <columnConstraints>
        <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0"/>
    </columnConstraints>
    <rowConstraints>
        <RowConstraints maxHeight="-Infinity" minHeight="-Infinity" vgrow="SOMETIMES"/>
        <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
    </rowConstraints>
    <children>
        <HBox alignment="CENTER_LEFT" spacing="5.0">
            <children>
                <Label text="If "/>
                <ComboBox fx:id="comboBoxConditionType" prefWidth="150.0"/>
                <Label text="matches:"/>
            </children>
        </HBox>
        <AnchorPane fx:id="anchorPaneContent" prefHeight="200.0" prefWidth="200.0" GridPane.rowIndex="1"/>
    </children>
    <padding>
        <Insets bottom="16.0" left="6.0" right="16.0" top="16.0"/>
    </padding>
</GridPane>
